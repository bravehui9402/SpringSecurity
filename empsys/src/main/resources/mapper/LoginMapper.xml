<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lh.empsys.common.mapper.LoginMapper">

    <select id="findUserByLoginName" parameterType="com.lh.empsys.user.pojo.UserInfo" resultType="com.lh.empsys.user.pojo.UserInfo">
        select * from sys_user su where su.login_name =  #{loginname}
    </select>


</mapper>